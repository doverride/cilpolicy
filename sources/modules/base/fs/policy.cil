	(blockinherit fs_obj_blk)

	(call sys_mountpoint_obj_type (fs)))

(block proc
	(blockinherit fs_obj_blk)

	(call sys_mountpoint_obj_type (fs))

	(call fs_associate_fs (fs)))

(block pstore
	(blockinherit fs_obj_blk)

	(call sys_mountpoint_obj_type (fs)))

(block removable_fs
	(blockinherit fs_obj_blk))

(block rootfs
	(blockinherit fs_obj_blk)

	(call sys_mountpoint_obj_type (fs))

	(call fs_associate_fs (fs)))


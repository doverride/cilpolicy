	(blockinherit sec_bool_obj_blk))

(block fs_use_removable
	(blockinherit sec_bool_obj_blk))

(block aio
	(blockinherit fs_obj_blk))

(block anon_inodefs
	(blockinherit fs_obj_blk)

	(call mls_trusted_obj_type (fs)))

(block autofs_fs
	(blockinherit fs_obj_blk)

	(call sys_mountpoint_obj_type (fs)))

(block bdev
	(blockinherit fs_obj_blk))

(block binfmt_misc
	(blockinherit fs_obj_blk)

	(call sys_mountpoint_obj_type (fs)))

(block cifs
	(blockinherit fs_obj_blk)

	(call sys_mountpoint_obj_type (fs)))

